package main

type FType =
| FInt
| FString
| FUnit
| FUnresolved
| FFunc of FuncType
| FRecord of RecordType
| FUnion of UnionType


type NameTypePair = {Name: string; Type: FType}

type RecordType = {name: string; fiedls: []NameTypePair}
type UnionType = {name: string; cases: []NameTypePair}

type FuncType = {targets: []FType}

let IsUnresolved (ft:FType) =
  match ft with
  | FUnresolved -> true
  | _ -> false

